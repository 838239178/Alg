#include <bits/stdc++.h>
using namespace std;

void QuickSort(int a[], int i, int j);

int main()
{
	int n;
	cin >> n;
	int* ans = new int[n];
	for (int i = 0; i < n; i++) cin >> ans[i];
	QuickSort(ans, 0, n - 1);
	for (int i = 0; i < n; i++) cout << ans[i];
	delete[]ans;
	return 0;
}
void QuickSort(int a[], int i, int j)
{
	int tj = j;                                
	int ti = i;
	int std = a[i];                           //定义标准值为区间第一个
	if (i >= j) return;
	while (i != j) {
		while (j != i) {                      //把std右边比std小的数复制到左边
			if (std > a[j]) {
				a[i] = a[j];
				break;
			}
			j--;
		}
		while (i != j) {					  //把std左边比std大的数复制到右边
			if (std < a[i]) {
				a[j] = a[i];
				break;
			}
			i++;
		}
	}
	a[i] = std;                                //把最后的位置（中间i==j) 设为std
	QuickSort(a, ti, i - 1);                   //排序左半区间
	QuickSort(a, i + 1, tj);                   //排序右半区间
}
